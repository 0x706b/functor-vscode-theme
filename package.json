{
   "name": "functor-vscode-theme",
   "displayName": "Functor",
   "description": "An aesthetically-pleasing utilitarian theme",
   "version": "0.0.1",
   "engines": {
      "vscode": "^1.44.0"
   },
   "scripts": {
      "watch": "tsnd --respawn ./src/functor.ts",
      "build": "tsnd ./src/functor.ts"
   },
   "categories": [
      "Themes"
   ],
   "contributes": {
      "themes": [
         {
            "label": "Functor",
            "uiTheme": "vs-dark",
            "path": "./themes/functor-color-theme.json"
         }
      ]
   },
   "dependencies": {},
   "devDependencies": {
      "@types/node": "^13.13.0",
      "@types/vscode": "^1.44.0",
      "ts-node-dev": "^1.1.8",
      "typescript": "^3.8.3"
   }
}
